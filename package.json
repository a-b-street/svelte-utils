{
  "name": "svelte-utils",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepare": "npm run package",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "fmt": "prettier --write ."
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    },
    "./overpass": {
      "types": "./dist/overpass/index.d.ts",
      "svelte": "./dist/overpass/index.js",
      "import": "./dist/overpass/index.js"
    },
    "./map": {
      "types": "./dist/map/index.d.ts",
      "svelte": "./dist/map/index.js",
      "import": "./dist/map/index.js"
    },
    "./two_column_layout": {
      "types": "./dist/two_column_layout/index.svelte.d.ts",
      "svelte": "./dist/two_column_layout/index.svelte.js",
      "import": "./dist/two_column_layout/index.svelte.js"
    },
    "./top_bar_layout": {
      "types": "./dist/top_bar_layout/index.svelte.d.ts",
      "svelte": "./dist/top_bar_layout/index.svelte.js",
      "import": "./dist/top_bar_layout/index.svelte.js"
    },
    "./three_column_layout": {
      "types": "./dist/three_column_layout/index.svelte.d.ts",
      "svelte": "./dist/three_column_layout/index.svelte.js",
      "import": "./dist/three_column_layout/index.svelte.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.26.0",
    "@sveltejs/package": "^2.4.0",
    "@sveltejs/vite-plugin-svelte": "^6.1.0",
    "@types/eslint": "^9.6.1",
    "@types/geojson": "^7946.0.16",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.11.0",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "publint": "^0.3.12",
    "svelte": "^5.36.16",
    "svelte-check": "^4.3.0",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "vite": "^7.0.6"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "dependencies": {
    "@turf/bbox": "^7.2.0",
    "maplibre-draw-polygon": "github:dabreegster/maplibre-draw-polygon",
    "maplibre-gl": "^5.6.1",
    "svelte-maplibre": "^1.2.0"
  }
}
